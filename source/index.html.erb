---
title: Soundboard
---

<div class="container">
  <div class="row">
    <% data.sounds.each do |sound| %>
      <div class="col-md-6">
        <div class="card">
          <div class="card-body">
            <h5 class="card-title"><%= sound.title %></h5>
            <audio controls src="sounds/<%= sound.file %>"></audio>
          </div>
        </div>
      </div>
      <% end %>
  </div>
</div>
